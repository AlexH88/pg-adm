.multi-select-field
  display flex
  flex-direction column
  align-items stretch
  padding 0
  margin 0

  &__header
    font-size 16px
    font-weight 600
    color #212121

  &__chipwrapper
    display flex
    align-items flex-start
    height 65px
    box-sizing border-box
    margin-left 8px
    padding-top 8px
    flex-direction column
    overflow-y scroll

  &__subwrapper
    flex 1 0 auto
    display flex
    flex-direction column
    align-items stretch